  echo $ac_n "(cached) $ac_c" 1>&4
else
  cat > conftest.$ac_ext <<EOF
#line 1404 "configure"
#include "confdefs.h"
#include <assert.h>
#include <stdio.h>
int main() { return 0; }
int t() {

/* check for requoting problems */
static int a, b, c, d;
static char *s;
assert (!strcmp(s, "foo bar baz quux"));
/* check for newline handling */
assert (a == b
        || c == d);

; return 0; }
EOF
if eval $ac_link; then
  rm -rf conftest*
  gas_cv_assert_ok=yes
